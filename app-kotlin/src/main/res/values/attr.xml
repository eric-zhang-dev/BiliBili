<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="rightPadding" format="dimension" />

    <declare-styleable name="FisrtSlidingMenu">
        <attr name="rightPadding" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="vpiTabPageIndicatorStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="ScalableImageView">
        <attr name="aspectRadioHeight" format="integer" />
        <attr name="aspectRadioWidth" format="integer" />
    </declare-styleable>
    <declare-styleable name="ForegroundView">
        <attr name="android:foreground" />
    </declare-styleable>
    <declare-styleable name="IndicatorTextView">
        <attr name="indicatorDrawable" format="reference" />
    </declare-styleable>
</resources>